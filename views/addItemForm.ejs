<form action="<%=route%>" method="post" >

<div>
<label for="name">Название:</label>
<input id="name" name="name" type="text"/>
</div>

<div>
<label for="about">Описание:</label>
<input id="about" name="about" type="text"/>
</div>

<div>
<label for="price">Цена:</label>
<input id="price" name="price" type="text"/>
</div>

<div>
<label for="tel">Телефон:</label>
<input id="tel" name="tel" type="text"/>
</div>

<div>
<label for="email">Почта:</label>
<input id="email" name="email" type="text"/>
</div>

<fieldset>
<legend>Выберите категорию:</legend>
<div>
<input type=radio id="all" name=tag value="all" checked/>
<label for="all">Общая</label>
</div>
<div>
<input type=radio id="auto" name=tag value="auto" />
<label for="auto">Авто</label>
</div>
</fieldset>

<div>

<input type="submit" value="Добавить"/>
</div>

</form>

